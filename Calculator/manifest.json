{
    "manifest_version": 2,
    "name": "Simple-Calculator",
    "version": "1.0",
    "description": "A Simple Calculator",

    "icons": {
        "48": "icon/Calculator_Logo.png"
    },

    "permissions": [
        "clipboardWrite"
    ],

    "browser_action": {
        "default_icon": "calculator_Logo.png",
        "default_title": "Simple-Calculator",
        "default_popup": "calculator.html",
        "browser_style": false
      }
    
}